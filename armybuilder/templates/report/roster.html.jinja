{% extends "report/base.html.jinja" %}
{% block content %}
<div class="roster-container">
<div class="kill-team-bio-container">
    <div class="bio-field-container">
        <div class="bio-label player-name">
            Player \n Name
        </div>
        <div class="bio-entry player-name">
            {{ player_name }}
        </div>
    </div>
<ul>
    <li>  Player Name : {{ player_name }}</li>
    <li>  Kill Team Name : {{ kill_team_name }}</li>
    <li>  Faction : {{ faction }}</li>
    <li>  Subfaction : {{ sub_faction }}</li>
    <li>  Points : {{ points }}</li>
</ul>
<table id="roster-table">
    {{ macros.roster_table_heading(models[0].keys()) }}
    {% for m in models %}
        {{ macros.roster_row(m) }}
    {% endfor %}

</table>
</div>
{% endblock %}